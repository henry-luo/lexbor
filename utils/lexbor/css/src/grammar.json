{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "css_grammar",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rule_definition"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        }
      ]
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "/*"
            },
            {
              "type": "PATTERN",
              "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
            },
            {
              "type": "STRING",
              "value": "/"
            }
          ]
        }
      ]
    },
    "rule_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "rule_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "rule_name": {
      "type": "PATTERN",
      "value": "<[a-zA-Z0-9-_]+>"
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "alternation"
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 2,
          "content": {
            "type": "SYMBOL",
            "name": "combinatorial"
          }
        },
        {
          "type": "PREC_LEFT",
          "value": 1,
          "content": {
            "type": "SYMBOL",
            "name": "sequence"
          }
        },
        {
          "type": "PREC",
          "value": 0,
          "content": {
            "type": "SYMBOL",
            "name": "atom"
          }
        }
      ]
    },
    "alternation": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": "|"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "combinatorial": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "expression"
          },
          {
            "type": "STRING",
            "value": "&&"
          },
          {
            "type": "SYMBOL",
            "name": "expression"
          }
        ]
      }
    },
    "sequence": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "atom"
      }
    },
    "atom": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "literal"
        },
        {
          "type": "SYMBOL",
          "name": "reference"
        },
        {
          "type": "SYMBOL",
          "name": "group"
        },
        {
          "type": "SYMBOL",
          "name": "repetition"
        },
        {
          "type": "SYMBOL",
          "name": "optional"
        }
      ]
    },
    "literal": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9-_]+"
    },
    "reference": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<"
        },
        {
          "type": "FIELD",
          "name": "name",
          "content": {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9-_]+"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT1",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": " "
                  },
                  {
                    "type": "FIELD",
                    "name": "constraint_name",
                    "content": {
                      "type": "PATTERN",
                      "value": "[a-zA-Z0-9-_]+"
                    }
                  },
                  {
                    "type": "STRING",
                    "value": "="
                  },
                  {
                    "type": "FIELD",
                    "name": "constraint_value",
                    "content": {
                      "type": "PATTERN",
                      "value": "-?[0-9]+(\\.[0-9]+)?"
                    }
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ">"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "^"
                },
                {
                  "type": "FIELD",
                  "name": "permission",
                  "content": {
                    "type": "PATTERN",
                    "value": "[A-Z]+"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "group": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "("
            },
            {
              "type": "SYMBOL",
              "name": "expression"
            },
            {
              "type": "STRING",
              "value": ")"
            }
          ]
        }
      ]
    },
    "repetition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "atom"
            },
            {
              "type": "STRING",
              "value": "*"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "atom"
            },
            {
              "type": "STRING",
              "value": "+"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "atom"
            },
            {
              "type": "STRING",
              "value": "#"
            }
          ]
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "atom"
            },
            {
              "type": "STRING",
              "value": "{"
            },
            {
              "type": "FIELD",
              "name": "min",
              "content": {
                "type": "PATTERN",
                "value": "[0-9]+"
              }
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SEQ",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "CHOICE",
                      "members": [
                        {
                          "type": "FIELD",
                          "name": "max",
                          "content": {
                            "type": "PATTERN",
                            "value": "[0-9]+"
                          }
                        },
                        {
                          "type": "BLANK"
                        }
                      ]
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "STRING",
              "value": "}"
            }
          ]
        }
      ]
    },
    "optional": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "atom"
        },
        {
          "type": "STRING",
          "value": "?"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "expression",
      "sequence"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}